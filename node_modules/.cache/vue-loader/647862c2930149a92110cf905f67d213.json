bject: null,\n    openCard: false\n  }),\n  computed: {\n    isLoading () {\n      return this.loading\n    },\n    hasError () {\n      return !this.loading && this.error\n    }\n  },\n  methods: {\n    proxySuccess (res) {\n      this.openCard = true\n      this.loading = false\n      this.reqConfig = res.data.requestConfig\n      if (res.data.error) {\n        this.error = true\n        this.errorMessage = res.data.errorMessage.message\n        this.errorObject = res.data.errorMessage.errorObj\n      } else {\n        this.error = false\n        this.resData = res.data.responseBody\n      }\n    },\n    applyApiCall (config) {\n      this.loading = true\n      axios(config).then(this.proxySuccess)\n    }\n  }\n}\n</script>\n\n<style>\n\n</style>"]}]}